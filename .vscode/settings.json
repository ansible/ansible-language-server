{
  "editor.formatOnSave": true,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/node_modules": true,
    "**/.task": true,
    "**/.venv": true,
    "**/out": true,
    "**/*.tgz": true,
  },
  "explorerExclude.backup": null,
  "mypy.configFile": ".config/mypy.ini",
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
    "*.jsx": "${capture}.js",
    "*.ts": "${capture}.js",
    "*.tsx": "${capture}.ts",
    ".gitignore": ".vscodeignore, .gitattributes, .prettierignore, .pre-commit-config.yaml, .readthedocs.yml, cspell.config.yml, codecov.yml, .prettierrc.yaml, cspell.config.yaml",
    "package.json": "package-lock.json, yarn.lock, tsconfig.json, .npmrc, .nycrc.yml, .eslintrc.json, .mocharc.yml, .nvmrc, icon.png, .editorconfig",
    "README.md": "CHANGELOG.md, LICENSE",
  },
}
